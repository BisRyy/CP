class Solution {
public:
    void sortColors(vector<int>& a) {
    
    int c = 0; bool f = true;
    while(f){
        f=false;
    for(int i=0; i<a.size()-1;i++){
        if(a[i]>a[i+1]){
            swap(a[i], a[i+1]);
            c++;f = true;
            
        }
    }}
    cout << "[";
    for(int i=0; i<a.size()-1;i++){
        cout << a[i];
        i == a.size()-1? cout<< "]":cout << ",";
    }
    }
};
